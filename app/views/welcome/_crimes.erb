<% @crimes.each do |crime| %>
  <a href="javascript:;" onclick="plotCrime(<%= crime.latitude %>, <%= crime.longitude %>, '<%= crime.primary %> - <%= crime.description %>', '<%= crime.date.asctime %>');" class="list-group-item">
    <h4><%= crime.primary %> - <%= crime.description %></h4>
    <p><%= crime.date.asctime %></p>
  </a>
<% end %>
